    {
      "comment": "=== COMPUTED BASIC FIELDS ===",
      "fields": [
        {
          "code": 5001,
          "name": "working_capital",
          "display_name": "Working Capital",
          "type": "currency",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "subtract",
            "args": [
              {"field": "total_current_assets"},
              {"field": "total_current_liabilities"}
            ]
          },
          "description": "Total Current Assets - Total Current Liabilities"
        }
      ]
    },
    {
      "comment": "=== ENTERPRISE VALUE METRICS ===",
      "fields": [
        {
          "code": 6001,
          "name": "ebitda",
          "display_name": "EBITDA",
          "type": "currency",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "add",
            "args": [
              {"field": "ebit"},
              {"field": "depreciation"}
            ]
          },
          "description": "EBIT + Depreciation"
        },
        {
          "code": 6002,
          "name": "ebitda_growth",
          "display_name": "EBITDA Growth",
          "type": "percentage",
          "category": "ratio",
          "is_computed": true,
          "description": "Year-over-year EBITDA growth rate (log)"
        },
        {
          "code": 6003,
          "name": "total_debt",
          "display_name": "Total Debt",
          "type": "currency",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "add",
            "args": [
              {"field": "long_term_debt"},
              {"field": "short_long_term_debt"}
            ]
          },
          "description": "Long Term Debt + Short Term Debt"
        },
        {
          "code": 6004,
          "name": "debt_ratio",
          "display_name": "Debt Ratio",
          "type": "ratio",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"field": "total_debt"},
              {"field": "total_assets"}
            ]
          },
          "description": "Total Debt / Total Assets"
        },
        {
          "code": 6005,
          "name": "excess_cash",
          "display_name": "Excess Cash",
          "type": "currency",
          "category": "ratio",
          "is_computed": true,
          "description": "Cash above working capital requirements"
        },
        {
          "code": 6006,
          "name": "market_cap",
          "display_name": "Market Capitalization",
          "type": "currency",
          "category": "market",
          "is_computed": true,
          "formula": {
            "op": "multiply",
            "args": [
              {"field": "shares_outstanding"},
              {"field": "avg_month_price"}
            ]
          },
          "description": "Shares Outstanding × Share Price"
        },
        {
          "code": 6007,
          "name": "earnings_per_share",
          "display_name": "Earnings Per Share",
          "type": "currency",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"field": "net_income"},
              {"field": "shares_outstanding"}
            ]
          },
          "description": "Net Income / Shares Outstanding"
        },
        {
          "code": 6008,
          "name": "price_to_earnings",
          "display_name": "Price to Earnings Ratio",
          "type": "ratio",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"field": "avg_month_price"},
              {"field": "earnings_per_share"}
            ]
          },
          "description": "Share Price / Earnings Per Share"
        },
        {
          "code": 6009,
          "name": "cost_of_debt",
          "display_name": "Cost of Debt",
          "type": "percentage",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"op": "multiply", "args": [{"field": "interest_expense"}, {"value": -1}]},
              {"field": "total_debt"}
            ]
          },
          "description": "Interest Expense / Total Debt"
        },
        {
          "code": 6010,
          "name": "leverage",
          "display_name": "Leverage",
          "type": "ratio",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"field": "total_assets"},
              {"op": "subtract", "args": [{"field": "total_assets"}, {"field": "total_liab"}]}
            ]
          },
          "description": "Total Assets / (Total Assets - Total Liabilities)"
        },
        {
          "code": 6011,
          "name": "enterprise_value",
          "display_name": "Enterprise Value",
          "type": "currency",
          "category": "market",
          "is_computed": true,
          "formula": {
            "op": "subtract",
            "args": [
              {"op": "add", "args": [{"field": "market_cap"}, {"field": "total_debt"}]},
              {"field": "excess_cash"}
            ]
          },
          "description": "Market Cap + Total Debt - Excess Cash"
        },
        {
          "code": 6012,
          "name": "ev_ebitda",
          "display_name": "EV/EBITDA",
          "type": "ratio",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"field": "enterprise_value"},
              {"field": "ebitda"}
            ]
          },
          "description": "Enterprise Value / EBITDA"
        },
        {
          "code": 6013,
          "name": "net_debt_ebitda",
          "display_name": "Net Debt/EBITDA",
          "type": "ratio",
          "category": "ratio",
          "is_computed": true,
          "formula": {
            "op": "divide",
            "args": [
              {"field": "total_debt"},
              {"field": "ebitda"}
            ]
          },
          "description": "Total Debt / EBITDA"
        }
      ]
    },
    {
